<!DOCTYPE html>
<html>
<head>
  <title>LibAdmin</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id = "user_nav">
    <% if current_user %>
      <%= current_user.name %>さん <%= link_to "ログアウト" ,logout_path %>
    <% else %>
      <%= link_to "Twitter Login", "/auth/twitter" %>
  <% end %>
  </div>
<%= yield %>

</body>
</html>
