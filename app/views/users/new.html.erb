<h1>User追加</h1>
<%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br />
  <%= f.label :kana %>
  <%= f.text_field :kana %><br />
  <%= f.label :birthday %>
  <%= f.text_field :birthday %><br />

  <%= f.fields_for :addresses do|f2| %>
    <%= f2.label :prefecture %>
    <%= f2.text_field :prefecture%><br />
  <% end %>
  <%= f.submit %>
<% end %>
<%= link_to 'Back', users_path %>
